<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/popup_imageView1"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:scaleType="fitStart"
        android:src="@mipmap/shouye">
    </ImageView>

    <ImageView
        android:id="@+id/popup_imageView2"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:scaleType="fitStart"
        android:src="@mipmap/sousuo">
    </ImageView>

    <ImageView
        android:id="@+id/popup_imageView3"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:scaleType="fitStart"
        android:src="@mipmap/shouchang">
    </ImageView>

    <ImageView
        android:id="@+id/popup_imageView4"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:scaleType="fitStart"
        android:src="@mipmap/zuji">
    </ImageView>

    <ImageView
        android:id="@+id/popup_imageView5"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:scaleType="fitStart"
        android:src="@mipmap/title_bar_share">
    </ImageView>
</LinearLayout>